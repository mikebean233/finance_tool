<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet id="types" author="author" runOnChange="true">
        <sqlFile dbms="mssql" path="sql/tables/types.sql" relativeToChangelogFile="true" splitStatements="true"/>
    </changeSet>
    <changeSet id="sources" author="author" runOnChange="true">
        <sqlFile dbms="mssql" path="sql/tables/sources.sql" relativeToChangelogFile="true" splitStatements="true"/>
    </changeSet>
    <changeSet id="categories" author="author" runOnChange="true">
        <sqlFile dbms="mssql" path="sql/tables/categories.sql" relativeToChangelogFile="true" splitStatements="true"/>
    </changeSet>
    <changeSet id="transactions" author="author" runOnChange="true">
        <sqlFile dbms="mssql" path="sql/tables/transactions.sql" relativeToChangelogFile="true" splitStatements="true"/>
    </changeSet>

</databaseChangeLog>
